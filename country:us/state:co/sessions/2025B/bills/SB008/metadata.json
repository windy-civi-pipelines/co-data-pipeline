  "identifier": "SB 008",
  "title": "Tech-Neutral Anti-Discrimination Clarification Act",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "Business & Economic Development",
